
@{
    ViewBag.Title = "SelezionaDestinatari";
}

<div class="col-lg-12">
    <div class="content-breadcrumb"><a href="/Home/Dashboard/">DASHBOARD</a> / <a href="/Corrispondenza/ProdottiPostali">CORRISPONDENZA</a> / <a href="/Telegramma">TELEGRAMMA</a> / SELEZIONA DESTINATARI</div>
</div>

<div class="default-page">
    <div class="col-lg-12">
        <section>
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h1>SCEGLI I DESTINATARI PER LA SPEDIZIONE</h1>
                    <h2>Seleziona i destinatari direttamente dalla tua rubrica oppure carica una nuova lista.</h2>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2 col-md-8 col-md-offset-2">
                    <div class="seleziona-destinatari">
                        <div class="col-lg-6 col-md-6">
                            <a href="/Telegramma/DestinatariList?Sender=@Request.QueryString["Sender"]&RicevutaRitorno=@Request.QueryString["RicevutaRitorno"]">
                                <img src="~/Images/IcnDestinatariList.png" width="88" /><br /><br />
                                Seleziona destinatari<br />
                                da rubrica
                            </a>
                        </div>
                        <div class="col-lg-6 col-md-6">
                            <a href="/Telegramma/DestinatariNew?Sender=@Request.QueryString["Sender"]&RicevutaRitorno=@Request.QueryString["RicevutaRitorno"]">
                                <img src="~/Images/IcnDestinatariNew.png" width="88" /><br /><br />
                                Carica una nuova<br />
                                lista di destinatari
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</div>


